package pt.ulisboa.tecnico.softeng.activity.domain;

class Activity {
	String name;
	String code;
	int minAge;
	int maxAge;
	int capacity;
}

relation DomainRootHasActivityApplication {
	.pt.ist.fenixframework.DomainRoot playsRole root {
		multiplicity 1..1;
	}
	Activity playsRole activity {
		multiplicity 0..*;
	}
}

